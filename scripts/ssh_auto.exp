#!/usr/bin/expect -f

set timeout 1200
set cmd [lindex $argv 0]
set password "1"
set host "xx@192.168.0.165"

spawn ssh -o StrictHostKeyChecking=no -o ConnectTimeout=5 $host $cmd

expect {
    "password:" {
        send "$password\r"
        exp_continue
    }
    "yes/no" {
        send "yes\r"
        exp_continue
    }
    eof
}

catch wait result
exit [lindex $result 3]
